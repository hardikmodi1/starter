    "@types/ioredis": "^4.0.13",
    "@types/jest": "^24.0.20",
    "@types/node": "^12.6.8",
    "@types/nodemailer": "^6.2.1",
    "@types/uuid": "^3.4.5",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.6.4"
