    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "faker": "^4.1.0",
    "graphql": "^14.4.2",
    "ioredis": "^4.14.0",
    "nodemailer": "^6.3.0",
    "pg": "^7.12.0",
    "reflect-metadata": "^0.1.13",
